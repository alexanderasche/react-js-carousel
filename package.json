{
  "name": "@alexcasche/react-carousel",
  "version": "1.0.0",
  "description": "React Carousel Component",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint src || exit 0",
    "test": "nodemon --exec mocha --require babel-core/register ./specs/*",
    "css": "node-sass ./src/styles/style.scss -o ./src/ && postcss ./src/style.css --use autoprefixer -d ./src",
    "watch": "nodemon -e scss -x \"npm run css\"",
    "dev": "concurrently \"webpack --config ./examples/webpack.config.js --mode development\" \"open ./examples/index.html\"",
    "gh-pages": "git subtree push --prefix examples origin gh-pages",
    "clean": "rimraf lib",
    "build": "cross-env babel src --out-dir lib",
    "copy": "cp src/style.css lib/",
    "prepare": "npm run clean && npm run build && npm run copy"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acasche/react-js-carousel"
  },
  "keywords": [
    "react",
    "carousel",
    "slider"
  ],
  "author": "Asche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acasche/react-js-carousel/issues"
  },
  "homepage": "https://github.com/acasche/react-js-carousel",
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "postcss-cli": "^5.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
